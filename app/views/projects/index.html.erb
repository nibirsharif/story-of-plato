<div class="container">
  <h1>Project Dashboard</h1>
  <section class="lists-container">
    <div class="list">
      <h3 class="list-title">Projects</h3>
      <% @projects.each do |project| %>
        <ul class="list-items">
          <%= link_to "#{project.name}", boards_path(project_id: project.id), class: "list-group-item list-group-item-action", method: :get %>
        </ul>
      <% end %>
    </div>
  </section>
</div>
